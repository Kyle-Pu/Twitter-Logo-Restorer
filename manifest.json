{
  "manifest_version": 3,
  "name": "Twitter Logo Changer",
  "version": "1.0",
  "description": "Replace Twitter logo with the OG logo.",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
    "web_accessible_resources": [{"resources": ["icon.png", "icon.svg"], "matches": ["https://twitter.com/*"]}],
    "permissions": ["tabs"],
    "host_permissions": ["https://twitter.com/*"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
